{
  "name": "reactron",
  "displayName": "Projeto base para uma aplicacao React/Electron/Material",
  "description": "Projeto base para uma aplicacao React/Electron/Material",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/ricardomansano/reactron",
  "publisher": "Mansano",
  "author": {
    "name": "Ricardo Mansano",
    "email": "ricardomansano@live.com"
  },
  "main": "app/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "start": "gulp",
    "dist": "gulp dist"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "babel-jest": "^27.0.6",
    "electron": "^9.4.4",
    "electron-builder": "^22.11.7",
    "electron-react-devtools": "^0.5.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-livereload": "^4.0.1",
    "jest": "^25.3.0",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.3.3",
    "ini": "^1.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "build": {
    "appId": "com.ricardomansano.rectron",
    "linux": {
      "target": [
        "dir"
      ]
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "category": "your.app.category.type"
    }
  }
}
